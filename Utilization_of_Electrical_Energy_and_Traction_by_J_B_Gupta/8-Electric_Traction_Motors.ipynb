"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Example 8.6.//power delivered\n",
"clc;\n",
"clear;\n",
"close;\n",
"//given data :\n",
"format('v',6)\n",
"Ft=35300;// in N\n",
"V=48;// in kmph\n",
"Po=((Ft*V*1000)/3600)*10^-3;\n",
"Ft1=55180;//in N\n",
"Pd=Po*sqrt(Ft1/Ft);\n",
"disp('part (a)')\n",
"disp(Pd,'power delivered(kW) = ')\n",
"Pd1=Po*(Ft1/Ft);\n",
"disp('part (b)')\n",
"disp(Pd1,'power delivered(kW) = ')"
   ]
   }
