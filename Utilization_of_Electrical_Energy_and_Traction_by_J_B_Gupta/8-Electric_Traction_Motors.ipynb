			"## Example 8.8: motor_speed.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Example 8.8: speed \n",
"clc;\n",
"clear;\n",
"close;\n",
"n1=500;//in rpm\n",
"d1=90;//in cm\n",
"d2=86;//in cm\n",
"v=600;//in volts\n",
"vd=0.1;//drop\n",
"eb1=v-(vd*v);//in volts\n",
"A=[90 -86;90 90];//\n",
"B=[240;54000];//\n",
"X=A\B;//\n",
"V1=X(1,1);//in volts\n",
"V2=X(2.1);//in volts\n",
"N1=n1*(V1-(vd*v))/(eb1);//\n",
"N2=N1*(d1/d2);//\n",
"disp(round(N1),' speed in rpm is')\n",
"disp(round(N2),'speed in rpm is')\n",
"//N2 is calculated wrong in the book"
   ]
