			"## Example 8.10: linear_synchronous_and_vehicle_speed.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Example 8.10;linear synchronous velocity\n",
"clc;\n",
"clear;\n",
"close;\n",
"f=50;//hz\n",
"t=0.5;//in meter\n",
"s=0.25;//\n",
"vs=2*f*t*(3600/1000);//kmph\n",
"vc=vs*(1-s);//kmph\n",
"disp(vs,'linear synchronous velocity in kmph is')\n",
"disp(vc,'vehicle speed in kmph is')"
   ]
   }
