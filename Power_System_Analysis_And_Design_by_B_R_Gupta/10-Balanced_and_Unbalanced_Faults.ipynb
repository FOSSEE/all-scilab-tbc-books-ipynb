"\n",
"Ia2g1= (Ia1a-Ia2g2)* exp(%i *-1*%pi /6) *-1\n",
"Ia2g1=round(Ia2g1 *1e2)/1e2\n",
"\n",
"Ia0g1= 0\n",
"\n",
"mIa1g1=[ Ia0g1 Ia1g1 Ia2g1]'\n",
"mIag1=A*mIa1g1\n",
"mIag1=round(mIag1*1e1)/1e1\n",
"Ibc=round(S*1e7/(sqrt(3) * V1 * 1e3))/10\n",
"Iag1=abs(mIag1) * Ibc\n",
"\n",
"\n",
"mprintf('\n\nline currents in A at generator 1')\n",
"mprintf('\nIa= %s', string(round(abs(Iag1(1))*1)/1)  )\n",
"mprintf('\nIb= %s', string(round(abs(Iag1(2))*1)/1) )\n",
"mprintf('\nIc= %s', string(round(abs(Iag1(3))*1)/1) )\n",
"\n",
"\n",
"Va1g1=round((1- (X(1,1) *%i * (Ia1a-Ia1g2)))*10000)/10000\n",
"Va1g1=Va1g1 *  exp(%i * 1*%pi /6) \n",
"Va2g1=round((0- (X(1,2) *%i * (Ia1a-Ia2g2)))*10000)/10000\n",
"Va2g1=Va2g1 *  exp(%i *- 1*%pi /6) \n",
"Va0g1=0\n",
"mVa1g1=[ Va0g1  Va1g1 Va2g1]'\n",
"mVag1=A*mVa1g1\n",
"vg1=mVag1 * V1 / sqrt(3)\n",
"\n",
"mprintf('\n\nline to neutral voltages in KV at generator 1 ')\n",
"mprintf('\nVa= %s', string(fix(abs(vg1(1))*100)/100)  )\n",
"mprintf('\nVb= %s', string(fix(abs(vg1(2))*100)/100) )\n",
"mprintf('\nVc= %s', string(fix(abs(vg1(3))*100)/100) )\n",
