			"## Example 9.12: rate_of_information.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Caption: rate of information\n",
"//Example 9.12\n",
"//page no 401\n",
"//Find Average rate of information\n",
"clc;\n",
"clear;\n",
"m=16;\n",
"pxi=1/16;\n",
"elements=2*10^6;\n",
"n=32\n",
"HX=0;\n",
"for(i=1:16)\n",
"   HX=HX+(-(pxi*log2(pxi)));\n",
"end\n",
"r=elements*n;\n",
"R=r*HX\n",
"printf('Average rate of information\n  \n \t R = %d Mbs',R/10^6);"
   ]
   }
