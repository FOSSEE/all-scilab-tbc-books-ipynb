"filename=pathname+filesep()+'4_23data.sci'\n",
"exec(filename)\n",
"t=5.2*x/Re^0.5;disp(t,'t=','boundary layer thickness t=5.2*x/Re^0.5')\n",
"D=q*S*Cf;disp(D,'D=','drag on one surface of the plate given by D=q*s*Cf')\n",
