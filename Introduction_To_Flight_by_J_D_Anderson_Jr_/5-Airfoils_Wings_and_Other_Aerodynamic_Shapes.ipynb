"pathname=get_absolute_file_path('5_01.sce')\n",
"filename=pathname+filesep()+'5_01data.sci'\n",
"exec(filename)\n",
"L=q*c*Cl;disp(L,'L=','L=q*c*Cl','Lift per unit span L:')\n",
"D=q*c*Cd;disp(D,'D=','D=q*c*Cd','Drag per unit span D:')\n",
