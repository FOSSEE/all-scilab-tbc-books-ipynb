"exec(filename)\n",
"T2=T1*(a2/a1)^1.5;\n",
"disp(T2,'T2=','T2=T1*(a2/a1)^1.5','period of mars T2 from keplers third law:')\n",
"printf('\Answer:\n')\n",
"printf('\n\Period of mars: %f days\n\n',T2)"
   ]
