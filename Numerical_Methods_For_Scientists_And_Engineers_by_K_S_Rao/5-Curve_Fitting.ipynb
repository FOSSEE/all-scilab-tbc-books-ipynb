"//Example 5.6\n",
"\n",
"clc\n",
"clear\n",
"\n",
"X = 1:0.2:2;\n",
"Y = [0.98 1.4 1.86 2.55 2.28 3.2];\n",
"\n",
"n = length(Y);\n",
"M1 = [sum(X.^4) sum(X.^3) sum(X.^2); sum(X.^3) sum(X.^2) sum(X); sum(X.^2) sum(X) n];\n",
"M2 = [sum(X.^2 .* Y); sum(X.*Y); sum(Y)];\n",
"A = M1\M2;\n",
"\n",
"a = A(1);\n",
"b = A(2);\n",
"c = A(3);\n",
"\n",
