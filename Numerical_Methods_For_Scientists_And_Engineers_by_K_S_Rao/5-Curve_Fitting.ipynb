			"## Example 5.1: Method_of_Group_Averages.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Example 5.1\n",
"clc\n",
"clear\n",
"\n",
"x = 10:10:80;\n",
"y = [1.06 1.33 1.52 1.68 1.81 1.91 2.01 2.11];\n",
"\n",
"X = log(x);\n",
"Y = log(y);\n",
"\n",
"n = length(Y);\n",
"M1 = [sum(Y); sum(X.*Y)];\n",
"M2 = [n sum(X); sum(X) sum(X.^2)];\n",
"\n",
"A = M2\M1;\n",
"\n",
"m = exp(A(1));\n",
"n = A(2);\n",
"\n",
"disp(round(m*10^4)/10^4, 'm =')\n",
"disp(round(n*10^4)/10^4, 'n =')"
   ]
   }
