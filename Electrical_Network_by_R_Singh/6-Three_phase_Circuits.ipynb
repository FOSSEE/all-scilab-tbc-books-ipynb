"VL=400;\n",
"Zph=100;\n",
"Vph=(VL/sqrt(3));\n",
"Iph=(Vph/Zph);\n",
"pf=1;\n",
"P=(sqrt(3)*VL*Iph*pf);\n",
"Iph1=(VL/Zph);\n",
"IL1=(sqrt(3)*Iph1);\n",
"P1=(sqrt(3)*VL*IL1*pf);\n",
"I1=(VL/200);\n",
