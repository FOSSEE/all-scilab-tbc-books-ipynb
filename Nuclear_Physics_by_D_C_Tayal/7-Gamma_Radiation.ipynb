	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"// Scilab code Exa7.14: : Page-297 (2011)\n",
"clc; clear;\n",
"g = 9.8;            // Acceleration due to gravity, metre per square sec\n",
"c = 3e+08;                // Velocity of light, metre per sec\n",
"y = 20;                // Vertical distance between source and absorber, metre\n",
