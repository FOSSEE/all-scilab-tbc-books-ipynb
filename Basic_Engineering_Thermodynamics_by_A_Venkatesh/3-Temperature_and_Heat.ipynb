		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clc\n",
"clear\n",
"printf('Example 3.5 | Page number 79 \n\n');\n",
"//Find the temperature attained by the coil during full load.\n",
"Rt = 80\n",
"t = 25\n",
"//Substituting Rt and t in Rt = Ro(1+0.00395t)\n",
"Ro = 80/(1+0.00395*25)\n",
"printf('Ro {Resistance at 0°C} = %.2f ohm\n',Ro)\n",
"//Full load condition\n",
"Rt = 95\n",
"//temperature at full load condition\n",
"printf('t {Temperature at full load condition} = %.2f °C',((Rt/Ro)-1)/0.00395);"
   ]
