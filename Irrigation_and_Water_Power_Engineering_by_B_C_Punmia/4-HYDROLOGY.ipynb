"Dc=(Cx^2+Cy^2);\n",
"Dd=(Dx^2+Dy^2);\n",
"De=(Ex^2+Ey^2);\n",
"Wb=1/Db;\n",
"Wc=1/Dc;\n",
"Wd=1/Dd;\n",
"We=1/De;\n",
"s=pB*Wb+pC*Wc+pD*Wd+pE*We;\n",
"pA=s/(Wb+Wc+Wd+We);\n",
"pA=round(pA*10)/10;\n",
"mprintf('precipitation at A=%f mm.',pA);"
