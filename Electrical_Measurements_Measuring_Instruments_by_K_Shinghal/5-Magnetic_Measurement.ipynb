  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Example 5.12 //hysteresis and eddy current components\n",
"clc;\n",
"clear;\n",
"close\n",
"w1=132;//W\n",
"f1=20;//Hz\n",
"x=w1/f1;//\n",
"w2=425;//W\n",
"f2=50;//Hz\n",
"y=w2/f2;//\n",
"A=[1 f1;1 f2];//\n",
"B=[x;y];//\n",
"X=A\B;//\n",
"Wh=X(1,1)*f2;//W\n",
"We=X(2,1)*f2^2;//W\n",
"disp(Wh,'hysteresis current component of the loss is,(W)=')\n",
"disp(We,'Eddy current component of the loss is,(W)=')"
   ]
   }
