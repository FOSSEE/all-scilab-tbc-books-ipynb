		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Example 5.9 // Magnetic pole difference\n",
"clc;\n",
"clear;\n",
"close;\n",
"//given data :\n",
"format('v',4)\n",
"N=150;// turns\n",
"I=1.2;// in A\n",
"theta=300;// divisions\n",
"t=155;//change in mmf in division\n",
"mmf=N*I;\n",
"r=2*mmf;// du to reversal\n",
"K=360/t;\n",
"M=(K*theta);\n",
"disp(M,'The magnetic potential difference between two points,(AT) = ')"
   ]
