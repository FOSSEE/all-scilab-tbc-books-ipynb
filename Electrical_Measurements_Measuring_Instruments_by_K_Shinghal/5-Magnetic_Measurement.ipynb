"source": [
"//Example 5.6 //flux per pole and leakage cofficient\n",
"clc;\n",
"clear;\n",
"close;\n",
"k=0.15;//micro-C\n",
"th=120;//divisions\n",
"th1=135;//divisions\n",
"r=4500;//ohm\n",
"n=3;//turns\n",
"ft=(k*10^-6*th*r)/n;//Wb\n",
"n1=4;//\n",
