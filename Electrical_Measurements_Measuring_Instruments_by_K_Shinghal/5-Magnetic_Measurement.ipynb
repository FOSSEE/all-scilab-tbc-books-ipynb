		   "source": [
			"## Example 5.15: loss.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Example 5.15 //loss\n",
"clc;\n",
"clear;\n",
"close\n",
"f=50;//Hz\n",
"k=2.3*10^-2;//\n",
"x=1.7;//\n",
"wi=0.6;//W\n",
"bm=0.5;//Wb/m^2\n",
"kd=((wi-(k*bm^x*f))/(bm^2*f^2));//\n",
"f1=20;//Hz\n",
"bm1=1;//\n",
"wi1=((k*bm1^x*f1)+(kd*bm1^2*f1^2));//\n",
"disp(wi1,'loss is ,(W)=')"
   ]
   }
