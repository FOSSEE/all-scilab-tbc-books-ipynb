		   "metadata": {},
		   "source": [
			"## Example 5.3_b: frequency.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Example 5.3.b:frequency\n",
"clc;\n",
"clear;\n",
"close;\n",
"//given data :\n",
"theta1=12.5;\n",
"theta2=10;\n",
"lamda=log(theta1/theta2);\n",
"x=lamda^2;//\n",
"y=x/(%pi^2-x);//\n",
"y1=sqrt(y);//\n",
"f=0.125;//Hz\n",
"fo=f/(sqrt(1-y1^2));//Hz\n",
"disp(fo,'undamped frequency is,(Hz)=')"
   ]
   }
