"i1=75;//mA\n",
"rsh2=((ra)/((i1/(ia*10^-3))-1));//in ohms\n",
"i3=100;//mA\n",
"ia3=0.4*ia;//micro amperes\n",
"rsh3=((ra)/((i3/(ia3*10^-3))-1));//in ohms\n",
"disp(rsh1,'shunt resistance when current is 10mA in ohm is')\n",
"disp(rsh2,'shunt resistance when current is 75mA in ohm is')\n",
