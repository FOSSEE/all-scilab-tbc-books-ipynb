"source": [
"//Example 4.15: Current \n",
"clc;\n",
"clear;\n",
"close;\n",
"//given data :\n",
"V1=4.2;// in V\n",
"V2=1.43;// in V\n",
"r1=21.0;// in ohm\n",
"r2=1;// in ohm\n",
"r3=15;// in ohm\n",
"I=V1/r1;// in A\n",
"R=V2/I;// in ohm\n",
"R1=R+r2;\n",
"R2=R-r2;\n",
"R3=round(R1*(r1-R1)/r1);\n",
"R4=R2*(r1-R2)/r1;\n",
"e1=R1*I;\n",
"e2=R2*I;\n",
"I1=(e1-V2)*10^3/(R3+r3);\n",
"I2=(V2-e2)*10^3/(R4+r3);\n",
"disp(I1,'Current in one direction,I1(mA) = ')\n",
"disp(I2,'Current in another direction,I2(mA) = ')"
   ]
