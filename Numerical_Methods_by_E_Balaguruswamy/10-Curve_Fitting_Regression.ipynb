"source": [
"//Example No. 10_05\n",
"//Plane Fitting\n",
"//Pg No. 342\n",
"clear ; close ; clc ;\n",
"\n",
"x = 1:4\n",
"z = 0:3\n",
"y = 12:6:30\n",
"n = length(x) //Number of data points\n",
"m = 3         //Number of unknowns\n",
"G = [ ones(1,n) ; x ; z]\n",
"H = G'\n",
"C = G*H\n",
"B = y*H\n",
"D = C\B'\n",
"disp(C,B)\n",
"disp(D)\n",
"mprintf('\n The regression plane is \n y = %i + %f*x + %i*z ',D(1),D(2),D(3))"
   ]
