"//solution\n",
"//given\n",
"n1=3\n",
"n2=2\n",
"n=4\n",
"d1=240//mm\n",
"r1=120//mm\n",
"d2=120//mm\n",
"r2=60//mm\n",
"u=0.3\n",
"P=25000//W\n",
"N=1575//rpm\n",
"T=P*60/(2*%pi*N)*1000//N-mm\n",
