"we=1.25//wear on each spring\n",
"Twe=we*csos/1000//total wear\n",
"Ss=13000//N/m//stiffness of spring\n",
"Rsf=Twe*Ss*ns//reduction\n",
"W1=W-Rsf\n",
"R1=(r1+r2)/2000\n",
"T=n*u*W1*R1//N-m\n",
"P=T*2*%pi*N/60//W\n",
"printf('power trans is,%f W\n',P)"
