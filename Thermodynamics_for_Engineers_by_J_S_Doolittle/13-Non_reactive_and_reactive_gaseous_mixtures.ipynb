		   "source": [
			"## Example 13.13: Weight_of_dry_air_calculations.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clc\n",
"clear\n",
"//Initialization of variables\n",
"c=0.74\n",
"ref=0.02\n",
"co2=0.12\n",
"co=0.1/100\n",
"o2=0.065\n",
"M=12\n",
"x=0.79\n",
"M=28.97\n",
"//calcualtions\n",
"n2=1-(co2+co+o2)\n",
"mol=n2/x\n",
"wt=mol*M\n",
"wt2=0.496\n",
"pou=wt2*wt\n",
"ta=10.27\n",
"EA=(pou-ta)/ta *100\n",
"//results\n",
"printf('Weight of air per pound of fuel = %.2f lb',pou)\n",
"printf('\n Excess air percentage = %.1f percent',EA)"
   ]
   }
