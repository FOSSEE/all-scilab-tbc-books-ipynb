			"## Example 13.10: volumetric_analysis.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clc\n",
"clear\n",
"//Initialization of variables\n",
"A=[1 1; 0.5 1]\n",
"B=[1; 0.9]\n",
"x=0.9\n",
"//calculations\n",
"N2=x*79/21\n",
"C=A\B\n",
"vec= [ C(1) C(2) N2]\n",
"su=sum(vec)\n",
"vec2=vec/su *100\n",
"//results\n",
"printf('Volumetric analysis')\n",
"disp('CO      CO2      N2')\n",
"disp(vec2)"
   ]
   }
