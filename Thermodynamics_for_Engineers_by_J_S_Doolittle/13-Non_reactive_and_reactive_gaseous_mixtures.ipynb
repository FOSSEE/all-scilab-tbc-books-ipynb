		   "source": [
			"## Example 13.5: Weight_calculations.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clc\n",
"clear\n",
"//Initialization of variables\n",
"x=[0.15 0.08 0.77]\n",
"M=[44 32 28]\n",
"//calculations\n",
"y=x ./M\n",
"yt=sum(y)\n",
"mt=y/yt\n",
"per=mt*100\n",
"wt=1/yt\n",
"R=1545/wt\n",
"//results\n",
"printf('Volumetric analysis')\n",
"disp('percent by volume')\n",
"format('v',6);per\n",
"disp(per)\n",
"printf('Weight per mole = %.1f lb',wt)\n",
"printf('\n Gas constant = %.1f ',R)"
   ]
