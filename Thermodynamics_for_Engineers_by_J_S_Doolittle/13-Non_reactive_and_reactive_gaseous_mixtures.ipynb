{
		   "cell_type": "markdown",
		   "metadata": {},
		   "source": [
			"## Example 13.9: volumetric_analysis.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clc\n",
"clear\n",
"//Initialization of variables\n",
"x=18.5 //Moles of O2\n",
"c=12 //Moles of CO2\n",
"vap=13 //moles of H2O\n",
"P=15 //psia\n",
"R=1545 //Universal gas constant\n",
"//calculations\n",
"excess=x*0.5\n",
"M=12*12+2*vap\n",
"n2=(x+excess)*79/21\n",
"nt=n2+excess+c\n",
"dry=[c x/2 n2]/nt *100\n",
"wet=nt+vap\n",
"fue=100/(M)\n",
"mol=wet*fue\n",
"vol=mol*R*1460/(144*P)\n",
"//results\n",
"disp('Volumetric analysis in percentage')\n",
"disp('       CO2         O2          N2')\n",
"disp(dry)\n",
"printf('Volume of wet products = %d cfm',vol)"
   ]
   }
