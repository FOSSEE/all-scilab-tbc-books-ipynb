"source": [
"//Example 9.8.b //final charging current\n",
"clc;\n",
"clear;\n",
"close\n",
"n=50;// no. of cells\n",
"Vc=250;// in volts\n",
"Vd=1.8;//in volts\n",
"Vcs=2.2;//in volts\n",
"r=0.01;//internal resistance of each cell in ohms\n",
"rl=0.1;//lead resistance in ohms\n",
"Re=19.4;//external resitance in ohms\n",
