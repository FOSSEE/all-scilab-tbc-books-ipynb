			"## Example 9.4_a: internal_resistance.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Example 9.4.a // internal resistance of each cell\n",
"clc;\n",
"clear;\n",
"close;\n",
"n=10;// no. of cells\n",
"Rl=4;// LOAD RESISTANCE\n",
"V=12;// in volts\n",
"Va=18;// IN VOLTS\n",
"r=((Va-V)*Rl)/(n*V);// internal resistance in ohms\n",
"disp(r,'internal resistance in ohms is')"
   ]
   }
