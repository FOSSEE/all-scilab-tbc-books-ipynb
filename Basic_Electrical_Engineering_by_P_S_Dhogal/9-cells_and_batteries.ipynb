			"## Example 9.7_a: ampere_hour_efficiency.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Example 9.7.a //find ampere-hour efficiency\n",
"clc;\n",
"clear;\n",
"close;\n",
"I=30;// in amperes\n",
"t=6;// in hours\n",
"Vt=2;// terminal voltage\n",
"Ic=40;// in amperes\n",
"tc=5;// in hours\n",
"Vc=2.5;// in volts\n",
"Aho=I*t;// ampere hour output of the battery\n",
"Ahi=Ic*tc;// ampere hour input of the battery\n",
"nAh=(Aho/Ahi)*100;// ampere hour efficiency\n",
"disp(nAh,'ampere hour efficiency of the battery in percentage is')"
   ]
   }
