			"## Example 9.5: emf_of_each_cell_and_its_internal_resistance.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Example 9.5 // emf and internal resistance of each cell\n",
"clc;\n",
"clear;\n",
"close;\n",
"n=6;// no. of cells\n",
"Rl=3;// LOAD RESISTANCE\n",
"I=2.5;// IN AMPERES\n",
"r1=9;// in ohms\n",
"I2=1.25;// om amperes\n",
"r=((r1*I2)-(Rl*I))/(n*(I-I2));// internal resistance in ohms\n",
"E=((I*(Rl+n*r))/n);// emf of each cell in volts\n",
"disp(E,'emf of each cell in volts is')\n",
"disp(r,'internal resistance of each cell in ohms')"
   ]
