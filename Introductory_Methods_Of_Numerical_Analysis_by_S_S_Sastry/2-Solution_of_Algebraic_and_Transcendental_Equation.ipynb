"//example 2.6\n",
"//false position method\n",
"//page 28\n",
"clc;clear;close\n",
"deff('y=f(x)','y=x^3-2*x-5');\n",
"a=2,b=3;//f(2) is negative and f(3)is positive\n",
"d=0.00001;\n",
" printf('succesive iterations    \ta\t    b\t    f(a)\t    f(b)\t\  x1\n');\n",
"for i=1:25\n",
"    x1=b*f(a)/(f(a)-f(b))+a*f(b)/(f(b)-f(a));\n",
"    if(f(a)*f(x1))>0\n",
"        b=x1;\n",
"    else\n",
"        a=x1;\n",
"    end\n",
"    if abs(f(x1))<d\n",
