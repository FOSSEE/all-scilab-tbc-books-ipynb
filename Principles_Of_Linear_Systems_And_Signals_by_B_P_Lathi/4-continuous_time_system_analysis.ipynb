			"## Example 4.9: initial_and_final_value.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Initial and final Value Theorem of Lapalace Transform\n",
"syms s;\n",
"num =poly([30 20],'s','coeff')\n",
"den =poly([0 5 2 1],'s','coeff')\n",
"X = num/den\n",
"disp (X,'X(s)=')\n",
"SX = s*X;\n",
"Initial_Value =limit(SX,s,%inf);\n",
"final_value =limit(SX,s,0); \n",
"disp(Initial_Value,'x(0)=')\n",
"disp(final_value,'x(inf)=')"
   ]
   }
