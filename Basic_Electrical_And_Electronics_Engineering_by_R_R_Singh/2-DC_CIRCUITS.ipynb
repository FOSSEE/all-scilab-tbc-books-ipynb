"source": [
"//Chapter 2,Example2.43,Pg 2.54\n",
"clc;\n",
"disp('Refer to the diagram shown in the question')\n",
"A=[7 -1;1 -6]\n",
"B=[10;0]\n",
"I=A\B\n",
"Vth=(3*I(2))-20\n",
"printf('\n Vth=%.2f V \n',Vth)\n",
"R1=[((6*1)/(6+1))+2]\n",
"Rth=R1*3/(R1+3)\n",
