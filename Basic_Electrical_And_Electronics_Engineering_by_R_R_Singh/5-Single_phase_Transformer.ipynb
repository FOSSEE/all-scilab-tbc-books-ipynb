"//Chapter5,Ex5.17,Pg5.19\n",
"clc;\n",
"E1=230 //EMF in primary winding\n",
"E2=460 //EMF in secondary winding\n",
"R1=0.2 //Primary resistance\n",
"R2=0.75 //Secondary resistance\n",
"X1=0.5 //Reactance in ohms\n",
