"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clear;\n",
"clc;\n",
"//Example 7.7\n",
"Vbe=0.7;\n",
"b=100;\n",
"Re=.5;\n",
"Rc=5;\n",
"Rl=10;\n",
"R1=40;\n",
"Cc=10;\n",
