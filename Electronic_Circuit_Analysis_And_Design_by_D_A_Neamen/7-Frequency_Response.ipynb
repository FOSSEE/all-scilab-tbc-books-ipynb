"printf('\nMiller capacitance=%.3f pF\n',Cm)\n",
"Cm=527*10^-3;\n",
"x=Rb*Rs/(Rb+Rs);\n",
"y=r*x/(r+x);\n",
"fH=1/(2*%pi*y*(C1+Cm));\n",
"printf('\nupper corner frequency =%.2fMHz\n',fH)\n",
"t=Rb*r/(Rb+r);\n",
"p=Rc*Rl/(Rc+Rl);\n",
"Av=gm*p*t/(t+Rs);\n",
"printf('\nmidband gain=%.2f\n',Av)"
