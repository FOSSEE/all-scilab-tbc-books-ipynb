		   "source": [
			"## Example 17.9: Calculate_the_currents_and_voltages_for_the_basic_TTL_NAND_circuit.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clear;\n",
"clc;\n",
"//Example 17.9\n",
"bf=25;\n",
"b=bf;\n",
"br=0.1;\n",
"Vcc=5;\n",
"R1=4;\n",
"Vbc=0.7;\n",
"Vy=0.1;\n",
"Vx=0.1;\n",
"R2=1.6;\n",
"Vbe=0.8;\n",
"Rc=4;\n",
"Vce=0.1;\n",
"vB2=Vx+Vce;\n",
"printf('\nvB2=%.2f V\n',vB2)\n",
"vB1=Vx+Vbe;\n",
"printf('\nbase voltage=%.2f V\n',vB1)\n",
"i1=(Vcc-vB1)/R1;\n",
"printf('\ncurrent i1=%.2f mA\n',i1)\n",
"vB1=Vbe+Vbe+Vbc;\n",
"printf('\nvB1=%.2f V\n',vB1)\n",
"vC2=Vbe+Vce;\n",
"printf('\ncollector voltage=%.2f V\n',vC2)\n",
"i1=(Vcc-vB1)/R1;\n",
"printf('\ncurrent i1 =%.2fmA\n',i1)\n",
"iB2=(1+2*br)*i1;\n",
"printf('\niB2=%.2f mA\n',iB2)\n",
"i2=(Vcc-vC2)/R2;\n",
"printf('\ni2 =%.2fmA\n',i2)\n",
"iE2=i2+iB2;\n",
"printf('\niE2=%.2f mA\n',iE2)\n",
"Rb=1;\n",
"i4=Vbe/Rb;\n",
"printf('\ncurrent in the pull down resistor=%.2f mA\n',i4)\n",
"iBo=iE2-i4;\n",
"printf('\nbase drive to the output transistor=%.2f mA\n',iBo)\n",
"i1=(Vcc-Vce)/Rc;\n",
"printf('\ni1=%.2f mA\n',i1)"
   ]
