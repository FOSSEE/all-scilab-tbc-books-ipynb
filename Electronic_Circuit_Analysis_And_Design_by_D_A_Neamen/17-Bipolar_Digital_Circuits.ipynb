			"## Example 17.12: Calculate_the_power_dissipation_in_a_low_power_Schottky_TTL_circuit.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clear;\n",
"clc;\n",
"//Example 17.12\n",
"Vy=0.3;\n",
"Vbe=0.7;\n",
"vx=0.4;\n",
"R2=8;\n",
"Vce=0.4;\n",
"Vcc=5;\n",
"b=25;\n",
"Vce=0.4;\n",
"Vbe1=0.7;\n",
"Vbe2=0.7;\n",
"Vcc=5;\n",
"R1=20;\n",
"v1=Vce+Vy;\n",
"i1=(Vcc-v1)/R1;\n",
"printf('\ni1=%.3f mA\n',i1)\n",
"Pl=i1*(Vcc-vx);\n",
"printf('\npower dissipation=%.3f mW\n',Pl)\n",
"v1=Vbe1+Vbe2;\n",
"printf('\nv1=%.3f V\n',v1)\n",
"vC2=Vbe1+Vce;\n",
"printf('\nvoltage vC2 =%.2fV\n',vC2)\n",
"i1=(Vcc-v1)/R1;\n",
"printf('\ncurrent i1 =%.3fmA\n',i1)\n",
"i2=(Vcc-vC2)/R2;\n",
"printf('\ncurrent i2 =%.3fmA\n',i2)\n",
"P=(i1+i2)*Vcc;\n",
"printf('\npower dissipation for high input condition=%.3f mW\n',P)"
   ]
   }
