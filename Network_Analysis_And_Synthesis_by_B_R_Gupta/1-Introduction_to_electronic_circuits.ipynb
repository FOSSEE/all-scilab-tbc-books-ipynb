"                    //EXAMPLE 1-30            PG NO-28\n",
"Q=80*10^-4;   //COULUMB CHARGR\n",
"C=150*10^-6;\n",
"C1=100*10^-6;  //CAPACITANCE\n",
"C2=50*10^-6;\n",
"Va=Q/C1;\n",
"Vb=Q/C2;\n",
