			"## Example 4.1: To_Find_the_Velocity_and_Acceleration.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//To Find the Velocity and Acceleration\n",
"clc\n",
"//Given:\n",
"N=120 //rpm\n",
"r=1,x=0.75 //m\n",
"//Solution:\n",
"//Calculating Angular Velocity\n",
"omega=2*%pi*N/60 //rad/s\n",
"//Calculating Velocity of the Piston\n",
"v=omega*sqrt(r^2-x^2) //m/s\n",
"//Calculating Acceleration of the Piston\n",
"a=omega^2*x\n",
"//Results:\n",
"printf('\n\n The Velocity of the Piston, v = %.2f m/s.\n',v)\n",
"printf(' The Acceleration of the Piston, a = %.2f m/s^2.\n\n',a)"
   ]
   }
