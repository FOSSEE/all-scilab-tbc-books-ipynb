			"## Example 4.11: To_Find_the_Mass_Moment_of_Inertia.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//To Find the Mass Moment of Inertia\n",
"clc\n",
"//Given:\n",
"m1=5.5,m2=1.5 //kg\n",
"l=1.25,r=125*10^-3 //m\n",
"//Solution:\n",
"//Calculating the Frequency of Oscillation\n",
"n=10/30 //Hz\n",
"//Calculating the Radius of Gyration About an Axis Through the c.g.\n",
"kG=r/(2*%pi*n)*sqrt(9.81/l) //m\n",
"//Calculating the Mass Moment of Inertia About an Axis Through its c.g.\n",
"m=m1+m2 //Total Mass, kg\n",
"I=m*kG^2 //kg-m^2\n",
"//Results:\n",
"printf('\n\n The Mass Moment of Inertia About an Axis Through its c.g., I = %.3f kg-m^2.\n\n',I)"
   ]
   }
