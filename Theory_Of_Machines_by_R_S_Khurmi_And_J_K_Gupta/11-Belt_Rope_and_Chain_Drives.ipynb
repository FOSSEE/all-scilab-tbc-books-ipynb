"source": [
"//To find number of ropes required\n",
"clc\n",
"//Given:\n",
"P=600 //kW\n",
"d=4 //m\n",
"N=90 //rpm\n",
"theta=160*%pi/180 //radians\n",
"funcprot(0)\n",
"beta=45/2 //degrees\n",
"mu=0.28\n",
"m=1.5 //kg/m\n",
