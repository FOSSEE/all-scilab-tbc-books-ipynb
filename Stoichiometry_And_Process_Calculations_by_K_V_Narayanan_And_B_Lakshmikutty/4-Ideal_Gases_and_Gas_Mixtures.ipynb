"T = 300;//K\n",
"p = 400;//kPa\n",
"P3 = p * per3 / 100;\n",
"v = 10;//m^3\n",
"V2 = per2 * v / 100;\n",
"M1 = 16.032;\n",
"M2 = 30.048;\n",
"M3 = 28;\n",
