"c=100;//capacitance in micro farads\n",
"x=1/(r*c*10^-6);//variable\n",
"i=30;//current in mA\n",
"y=(i*10^-3)-(v/r1);//variable\n",
"t=-((log(y*(r/v))));//time in seconds\n",
"t1=t/x;//time in seconds\n",
"disp(t1,'time is ,(seconds)=')"
