"//chapter 7\n",
"//Example 7.9\n",
"//Page 190\n",
"//impedacedetermination\n",
"clear;clc;\n",
"//Given Impedances\n",
"Z10 = %i*1.2;\n",
"Z21 = %i*0.2;\n",
"Z23 = %i*0.15;\n",
"Z13 = %i*0.3;\n",
"Z30 = %i*1.5;\n",
"//Solution\n",
"Z_eq = (Z13 * (Z21+Z23) / (Z13+Z21+Z23));\n",
"Z11 = Z10 * (Z30 + Z_eq) / (Z10 + Z30 + Z_eq);\n",
"disp('Z11 is given by')\n",
"disp(Z11)"
