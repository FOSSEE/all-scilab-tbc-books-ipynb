			"## Example 7.1: busadmittancematrix.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//chapter 7\n",
"//Example 7.1\n",
"//Page 170\n",
"//busadmittancematrix\n",
"clear;clc;\n",
"//Given\n",
"//Voltage Sources\n",
"Ea = 1.5;\n",
"Eb = 1.5*(cos(-36.87 * %pi / 180) + %i * sin(-36.87 * %pi / 180))\n",
"Ec = 1.5;\n",
"//admittances\n",
"Ya = -%i*0.8;\n",
"Yb = Ya;\n",
"Yc= Ya;\n",
"Yd = -%i*5;\n",
"Ye = -%i*8;\n",
"Yf = -%i*4;\n",
"Yg = -%i*2.5;\n",
"Yh = Yd;\n",
"//current sourcs\n",
"I1 = Ea * Ya;\n",
"I2 = Eb * Yb;\n",
"I3 = I1;\n",
"I4 = 0;\n",
"disp('Current Sources are')\n",
"printf('\n I1 = - j%.2f per unit',-imag(I1))\n",
"printf('\n I2 = %.2f - j%.2f per unit',real(I2),-imag(I2))\n",
"printf('\n I3 = - j%.2f per unit \n\n',-imag(I3))\n",
"//Self-admittances\n",
"Y11 = Yd + Yf + Ya;\n",
"Y22 = Yh + Yg + Yb;\n",
"Y33 = Ye + Yc + Yg + Yf;\n",
"Y44 = Yd + Ye + Yh;\n",
"disp('Self-admittances are')\n",
"printf('\n Y11 = - j%.2f per unit',-imag(Y11))\n",
"printf('\n Y22 = - j%.2f per unit',-imag(Y22))\n",
"printf('\n Y33 = - j%.2f per unit',-imag(Y33))\n",
"printf('\n Y44 = - j%.2f per unit\n\n',-imag(Y44))\n",
"//Mutual-admittances\n",
"Y12 = 0;Y21 = Y12;\n",
"Y13 = -Yf;Y31 = Y13;\n",
"Y14 = -Yd;Y41 = Y14;\n",
"Y23 = -Yg;Y32 = Y23;\n",
"Y24 = -Yh;Y42 = Y24;\n",
"Y34 = -Ye;Y43 = Y34;\n",
"disp('Mutual admittances are')\n",
"printf('\n Y12 = Y21 = %.2f per unit',imag(Y12))\n",
"printf('\n Y13 = Y31 = j%.2f per unit',imag(Y13))\n",
"printf('\n Y14 = Y41 = j%.2f per unit',imag(Y14))\n",
"printf('\n Y23 = Y32 = j%.2f per unit',imag(Y23))\n",
"printf('\n Y24 = Y42 = j%.2f per unit',imag(Y24))\n",
"printf('\n Y34 = Y43 = j%.2f per unit \n\n',imag(Y34))\n",
"//Matrix Form\n",
"I = [I1 ; I2 ; I3 ; I4];\n",
"Y = [Y11 Y12 Y13 Y14;Y21 Y22 Y23 Y24;Y31 Y32 Y33 Y34;Y41 Y42 Y43 Y44];\n",
"disp('Current Vector =')\n",
"disp(I)\n",
"disp('Bus admittance matrix =')\n",
"disp(Y)"
   ]
   }
