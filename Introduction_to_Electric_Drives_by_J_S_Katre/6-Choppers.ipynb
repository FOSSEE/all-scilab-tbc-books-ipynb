"source": [
"//Example 6.9.2 :chopping frequency and new output voltage\n",
"clc;\n",
"clear;\n",
"close;\n",
"format('v',8)\n",
"//given data :\n",
"v=200;//in volts\n",
"vo=600;//in volts\n",
"ton=200;//in micro seconds\n",
"x=-((v/vo)-1);//\n",
"f=x/(ton*10^-6);//\n",
"ton1=ton/2;//\n",
"Vo=((v)/(1-(f*ton1*10^-6)));//in volts\n",
"disp(f,'chopping frequency in Hz is')\n",
"disp(Vo,'new output voltage in volts is')"
   ]
