"//Example 6.5.2: chooper efficiency,input resistance and average load current\n",
"clc;\n",
"clear;\n",
"close;\n",
"format('v',6)\n",
"//given data \n",
"r=10;//in ohms\n",
"f=1;//in kHz\n",
"t=1/f;//in ms\n",
"d=0.3;//\n",
"v=200;//\n",
"vch=2;//in volts\n",
"Po=((v-vch)^2)*(d/r);//in watts\n",
