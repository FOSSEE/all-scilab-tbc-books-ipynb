		   "source": [
			"## Example 2.7_1: chapter_2_example_5.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clc\n",
"pathname=get_absolute_file_path('2_7_1.sce')\n",
"filename=pathname+filesep()+'271.sci'\n",
"exec(filename)\n",
"//this program uses least squares fit to solve for slope and intercept.\n",
"//hence the value differs from textbook a bit.\n",
"sx=sum(x);sx2=sum(x^2);sy=sum(y);sxy=sum(x.*y);n=length(x);\n",
"A=[sx,n;sx2,sx];B=[sy;sxy];p=A\B;\n",
"m=p(1,1);b=p(2,1);\n",
"clf()\n",
"xtitle('2.7_1.sce','Vdot(L/min)','R','boxed')\n",
"plot2d(x,y,style=3)\n",
"disp('in case 2, R=36')\n",
"R=36;\n",
"V=m*R+b;\n",
"printf('then V=%f',V);"
   ]
   }
