"printf(' \n m2=%f Kg/h',m2)\n",
"disp('Using Toluene balance,')\n",
"mT3=(1-inputx)*m1-(1-yB2)*m2\n",
"printf(' \n mT3=%f Kg/h',mT3)\n",
"m3=mB3+mT3\n",
"printf(' \n m3=%f Kg/h',m3)\n",
"yB3=mB3/m3\n",
"printf(' \n yB3=%f kg B/kg',yB3)\n",
"yT3=1-yB3\n",
"printf(' \n yT3=%f kg T/kg',yT3)"
