	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clc;\n",
"clear;\n",
"//Example 5.6\n",
"printf('\nLMTD Approach\n')\n",
"\n",
"Cph=4187            //[J/(kg.K)]\n",
"mh_dot=600/3600     //Hot side flow rate [kg/s]\n",
"mc_dot=1500/3600        //[kg/s]\n",
"Cpc=Cph             //[J/kg.K]\n",
"T1=343              //[K]\n",
"T2=323              //[K]\n",
"Q=mh_dot*Cph*(T1-T2)    //[W]\n",
"t1=298                  //[K]\n",
"t2=(mh_dot*Cph*(T1-T2))/(mc_dot*Cpc)+t1 //[K]\n",
"dT1=45      //[K]\n",
"dT2=17          //[K]\n",
"dTlm=(dT1-dT2)/log(dT1/dT2)         //[K]\n",
"hi=1600         //Heat transfer coeff in [W/sq m.K]\n",
"ho=hi           //[W/sq m.K]\n",
"U=1/(1/hi+1/ho)     //[W/sq m.K]\n",
"A=Q/(U*dTlm)        //[sq m]\n",
"\n",
"printf('\nEffectiveness-NTU approach\n');\n",
"\n",
"//hot water:\n",
"h=mh_dot*Cph            //[W/K]\n",
