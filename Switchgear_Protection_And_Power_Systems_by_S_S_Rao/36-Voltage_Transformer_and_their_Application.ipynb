			"## Example 36.03: To_calculate_the_VA_of_the_output_of_voltage_transformer.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clear;\n",
"clc;\n",
"v=110;\n",
"x=.1;\n",
"i=.1;\n",
"Va=v*i+(i^2*x);\n",
"mprintf('the total volt ampers = %dVA',Va);"
   ]
   }
