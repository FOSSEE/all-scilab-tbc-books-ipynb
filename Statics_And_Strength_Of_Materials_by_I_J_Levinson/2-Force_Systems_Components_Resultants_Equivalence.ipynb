			"## Example 2.7: chapter_2_example_7.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clc\n",
"//initialisation of variables\n",
"Fy1= 2 //kips\n",
"Fy2= 5 //kips\n",
"Fy3= 10 //kips\n",
"Fy4= 3 //kips\n",
"L= 5 //ft\n",
"Ry= Fy1+Fy2+Fy3+Fy4\n",
"x= (Fy1*L+Fy2*2*L+Fy3*3*L+Fy4*4*L)/Ry\n",
"//RESULTS\n",
"printf ('Ry= %.2f kips',Ry)\n",
"printf (' \n x=%.1f ft to the right of O',x)"
   ]
