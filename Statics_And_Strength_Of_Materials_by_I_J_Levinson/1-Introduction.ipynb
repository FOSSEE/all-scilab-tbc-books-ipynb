			"## Example 1.4: chapter_1_example_4.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clc\n",
"//initialisation of variables\n",
"b= 12\n",
"angle1= 35 //degrees\n",
"angle2= 43 //degrees\n",
"//CALCULATIONS\n",
"angle3= 180-angle1-angle2\n",
"a= sind(angle2)*b/sind(angle3)\n",
"c= a*sind(angle1)/sind(angle2)\n",
"//RESULTS\n",
"printf ('c = %.2f ',c)\n",
"printf (' \a=%.2f.',a)"
   ]
   }
