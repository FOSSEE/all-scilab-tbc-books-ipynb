"printf(' \n  ___________________________________________________________________');\n",
"printf(' \n      Given \t\t\t Calculated \t Calculated \t\ ');\n",
"printf(' \n   a. %.1f + %.1f = %.1f \t\t %.3f \t\t %.1f ',R_r,R_x_a,R_r+R_x_a,s_r_a,S_r_a);\n",
"printf(' \n   b. %.1f + %.1f = %.1f \t\t %.3f \t\t %.1f ',R_r,R_x_b,R_r+R_x_b,s_r_b,S_r_b);\n",
