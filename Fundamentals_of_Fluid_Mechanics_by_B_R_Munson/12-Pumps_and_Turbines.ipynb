			"## Example 12.2: shaft_power_calculatio.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"clc;\n",
"clear;\n",
"exec('C:\Program Files\scilab-5.3.0\bin\TCP\12_2data.sci');\n",
"U2=r2*2*%pi*N/(60*12);//ft/sec\n",
"Vr2=(1400/(7.48*60*2*%pi*(r2/12)*(b/12)));//ft/sec\n",
"V2tangential=U2-(Vr2*cotg(beta2*%pi/180));//ft/sec\n",
"hi=U2*V2tangential/32.2;//ft\n",
"disp('ft',hi,'The ideal head rise=')\n",
"d=1.94;//slugs/(ft^3)\n",
"Wshaft=(d*Q*U2*V2tangential/(7.58*60))/550;//hp\n",
"disp('hp',Wshaft,'The power transferred to the fluid=')"
   ]
   }
