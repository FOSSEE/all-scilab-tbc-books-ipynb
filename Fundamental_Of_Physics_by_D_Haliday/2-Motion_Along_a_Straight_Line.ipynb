			"## Example 2.1_d: Sample_Problem_1d.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Given that\n",
"exec('Example2_1a.sce', -1)\n",
"exec('Example2_1a.sce', -1)\n",
"clc\n",
"\n",
"//Sample Problem 2-1d\n",
"printf('\n**Sample Probelm 2-1d**\n')\n",
"time_station = 45  //in min\n",
"//he trevels 2 km back to the truck\n",
"final_displacement = overall_displacement + 2  //in km\n",
"final_average_velocity = final_displacement /(delta_t + time_station /60)\n",
"printf('Average velocity from the begining of the drive till reaching back to the truck is %f km/h', final_average_velocity)"
   ]
