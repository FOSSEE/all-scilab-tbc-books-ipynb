"clc;clear;\n",
"//Example 12.1\n",
"\n",
"//given data\n",
"r=0.5;//ratio of mass of Pb206 and mass of U238\n",
"t=4.5*10^9;//half life in years\n",
"\n",
"//calculation\n",
"T=(log(1+r))*(t/0.693);\n",
"disp(T,'age in years')"
