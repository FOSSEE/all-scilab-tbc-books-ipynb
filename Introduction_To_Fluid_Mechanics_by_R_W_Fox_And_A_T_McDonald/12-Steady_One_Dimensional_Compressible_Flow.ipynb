"t1=0.5289;\n",
"//(T/T*)2:\n",
"t2=0.9119;\n",
"//(p/p*)1:\n",
"P1=0.3636;\n",
"//(p/p*)2:\n",
"P2=0.7958;\n",
"//(V/V*)1:\n",
"v1=1.455;\n",
"//(V/V*)2:\n",
