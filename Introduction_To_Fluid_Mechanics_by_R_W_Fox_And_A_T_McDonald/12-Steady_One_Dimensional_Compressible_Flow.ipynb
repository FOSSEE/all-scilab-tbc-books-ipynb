"source": [
"//number and flow//\n",
"pathname=get_absolute_file_path('12.05.sce')\n",
"filename=pathname+filesep()+'12.05-data.sci'\n",
"exec(filename)\n",
"//Mach number at the exit:\n",
"Me=sqrt(((p0/pe)^((k-1)/k)-1)*2/(k-1))\n",
"//Temperature at exit(in K):\n",
