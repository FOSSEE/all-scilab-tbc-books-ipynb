"printf('\n\nVelocity at section 2: %.3f m/sec\n\n',V2)\n",
"printf('\n\nDensity at section 2 : %.3f kg/m^3\n\n',d2)\n",
"printf('\n\nStagnation pressure at section 2: %.3f kPa\n\n',p02)\n",
"printf('\n\nChange in entropy: %.3f kg-K\n\n',dS)\n",
"printf('\n\nStagnation temperature at section 2: %.3f K\n\n',T02)"
