			"## Example 7.04: Example_4.sce"
		   ]
		  },
  {
"cell_type": "code",
	   "execution_count": null,
	   "metadata": {
	    "collapsed": true
	   },
	   "outputs": [],
"source": [
"//Problem 7.04: A flux density of 1.2 T is produced in a piece of cast steel by a magnetizing force of 1250 A/m. Find the relative permeability of the steel under these conditions.\n",
"\n",
"//initializing the variables:\n",
"B = 1.2; // in Tesla\n",
"H = 1250; // in A/m\n",
"Pi = 3.14;\n",
"u0 = 4*Pi*1E-7;\n",
"\n",
"//calculation:\n",
"ur = B/(u0*H)\n",
"\n",
"printf('\n\nResult\n\n')\n",
"printf('\n relative permeability of the steel = %.0f \n',ur)"
   ]
   }
