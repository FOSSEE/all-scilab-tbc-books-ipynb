"source": [
"//page 107\n",
"clear;\n",
"close;\n",
"clc;\n",
"A=[1 2;3 6];\n",
"disp(A,'A=');\n",
"[m,n]=size(A);\n",
"disp(m,'m=');\n",
"disp(n,'n=');\n",
"[v,pivot]=rref(A);\n",
"r=length(pivot);\n",
"disp(r,'rank=')\n",
"cs=A(:,pivot);\n",
"disp(cs,'Column space=');\n",
"ns=kernel(A);\n",
"disp(ns,'Null space=');\n",
"rs=v(1:r,:)';\n",
"disp(rs,'Row space=')\n",
"lns=kernel(A');\n",
"disp(lns,'Left null sapce=');"
   ]
